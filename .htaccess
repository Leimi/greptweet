DirectoryIndex index.shtml index.html
AddType text/cache-manifest .appcache
RewriteEngine on
RewriteRule ^f/(.*)$ /create.cgi?id=$1               [L,QSA]
<IfModule mod_speling.c>
	CheckCaseOnly On
</IfModule>

<IfModule mod_expires.c>
    ExpiresActive On
    ExpiresByType text/css "access plus 1 week"
</IfModule>
